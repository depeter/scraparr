# Environment configuration for Tripflow ETL

# Source database (Scraparr)
SCRAPARR_DB_URL=postgresql://scraparr:scraparr@localhost:5432/scraparr

# Target database (Tripflow)
# Note: Using port 5433 if running Tripflow DB in separate Docker container
TRIPFLOW_DB_URL=postgresql://tripflow:tripflow@localhost:5433/tripflow

# ETL Configuration
BATCH_SIZE=1000
ENABLE_DEDUPLICATION=true
DEDUP_DISTANCE_METERS=50